{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Server",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/trackerServer/dist/index.js",
      "outFiles": [
        "${workspaceFolder}/trackerServer/dist/**/*.js"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "args": []
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Client",
      "url": "http://localhost:8000",
      "webRoot": "/Users/tedshaffer/Documents/Projects/tracker/sbo/src",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*",
        "webpack:///./~/*": "${workspaceFolder}/node_modules/*"
      }
    }
  ]
}
